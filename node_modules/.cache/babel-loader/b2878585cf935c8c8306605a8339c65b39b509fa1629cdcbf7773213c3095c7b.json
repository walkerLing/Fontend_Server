n                    </el-table>\r\n                    </el-tab-pane>\r\n\r\n                  </el-tabs>\r\n                </el-card>\r\n              </div>\r\n            </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\n    name: 'BlockSearch',\r\n    data() {\r\n        return {\r\n            storehouse: {},\r\n            url_1:\"..\\assets\\images\\bkgWeather.png\",\r\n        };  \r\n    },\r\n    methods:{\r\n        // 加载仓储信息\r\n        loadStoreHouse(){\r\n            this.axios.get(\"http://fruit.mingbyte.com/gateway/fruit/queryChaincode?uuid=111\")\r\n            .then(res=>{\r\n                this.storehouse = res.data\r\n            })\r\n            .catch(function (error) {\r\n                alert(\"请求错误\")\r\n            })\r\n        },\r\n        loadPicture(){\r\n            var config = {\r\n                    method: 'get',\r\n                    url: 'fruit.mingbyte.com/gateway/fruit/image',\r\n                    headers: { \r\n                        'User-Agent': 'Apifox/1.0.0 (https://www.apifox.cn)'\r\n                        }\r\n                    };\r\n\r\n                    axios(config)\r\n                    .then(function (response) {\r\n                    console.log(JSON.stringify(response.data));\r\n                    })\r\n                    .catch(function (error) {\r\n                    console.log(error);\r\n                    });\r\n        }\r\n    },\r\n    mounted:{\r\n        load(){\r\n            loadPicture();\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.visualizationOfBlock{\r\n    flex: 6;\r\n    border-radius: 15px;\r\n    width:985px;\r\n    margin-left: 9%;\r\n}\r\n#info_patient {\r\n    margin-top: 10px;\r\n    margin-right: 160px;\r\n  }\r\n  .image_1 {\r\n    width: 275px;\r\n    height: 260px;\r\n    background: rgba(208, 206, 216, 0.5);\r\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n  }\r\n</style>"]},"metadata":{},"sou