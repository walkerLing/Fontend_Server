{"ast":null,"code":"// 局部引入\nimport EleUploadImage from 'vue-ele-upload-image';\nimport axios from 'axios';\nexport default {\n  name: 'BlockStorage',\n  components: {\n    EleUploadImage\n  },\n  data() {\n    return {\n      fileList01: [{\n        name: 'food.jpeg',\n        url: 'https://fuss10.elemecdn.com/3/63/4e7f3a15429bfda99bce42a18cdd1jpeg.jpeg?imageMogr2/thumbnail/360x360/format/webp/quality/100'\n      }, {\n        name: 'food2.jpeg',\n        url: 'https://fuss10.elemecdn.com/3/63/4e7f3a15429bfda99bce42a18cdd1jpeg.jpeg?imageMogr2/thumbnail/360x360/format/webp/quality/100'\n      }],\n      fileList02: [{\n        name: 'food.jpeg',\n        url: 'https://fuss10.elemecdn.com/3/63/4e7f3a15429bfda99bce42a18cdd1jpeg.jpeg?imageMogr2/thumbnail/360x360/format/webp/quality/100'\n      }],\n      headers: {\n        Authorization: localStorage.getItem(\"token\")\n      },\n      form: {\n        name: '',\n        region: '',\n        date1: '',\n        date2: '',\n        delivery: false,\n        type: [],\n        resource: '',\n        desc: '',\n        image: ''\n      }\n    };\n  }\n};","map":{"version":3,"mappings":"AAyDA;AACA;AACA;AACA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;QAAAJ;QAAAK;MAAA,GACA;QAAAL;QAAAK;MAAA;MACAC;QAAAN;QAAAK;MAAA;MACAE;QACAC;MACA;MACAC;QACAT;QACAU;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA;IACA;EACA;AACA","names":["name","components","EleUploadImage","data","fileList01","url","fileList02","headers","Authorization","form","region","date1","date2","delivery","type","resource","desc","image"],"sourceRoot":"src/components","sources":["BlockStorage.vue"],"sourcesContent":["<template>\r\n  <!-- 区块链溯源相关信息可视化展示区域 -->\r\n  <div class=\"visualizationOfBlock\">\r\n    <div id=\"info_patient\">\r\n      <!-- 卡片放置表格 -->\r\n      <el-card style=\"border-radius: 30px;background-color: rgba(20, 177, 212,0.2);width:877px;text-align:center;\">\r\n\r\n        <div slot=\"header\" class=\"clearfix\">\r\n          <span id=\"fonts\" style=\"color: white;\">存储信息表单如下</span>\r\n        </div>\r\n\r\n        <el-form ref=\"form\" :model=\"sizeForm\" label-width=\"80px\" size=\"mini\">\r\n          <el-form-item label=\"活动名称\">\r\n            <el-input v-model=\"sizeForm.name\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"活动区域\">\r\n            <el-select v-model=\"sizeForm.region\" placeholder=\"请选择活动区域\">\r\n              <el-option label=\"区域一\" value=\"shanghai\"></el-option>\r\n              <el-option label=\"区域二\" value=\"beijing\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"活动时间\">\r\n            <el-col :span=\"11\">\r\n              <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"sizeForm.date1\"\r\n                style=\"width: 100%;\"></el-date-picker>\r\n            </el-col>\r\n            <el-col class=\"line\" :span=\"2\">-</el-col>\r\n            <el-col :span=\"11\">\r\n              <el-time-picker placeholder=\"选择时间\" v-model=\"sizeForm.date2\" style=\"width: 100%;\"></el-time-picker>\r\n            </el-col>\r\n          </el-form-item>\r\n          <el-form-item label=\"活动性质\">\r\n            <el-checkbox-group v-model=\"sizeForm.type\">\r\n              <el-checkbox-button label=\"美食/餐厅线上活动\" name=\"type\"></el-checkbox-button>\r\n              <el-checkbox-button label=\"地推活动\" name=\"type\"></el-checkbox-button>\r\n              <el-checkbox-button label=\"线下主题活动\" name=\"type\"></el-checkbox-button>\r\n            </el-checkbox-group>\r\n          </el-form-item>\r\n          <el-form-item label=\"特殊资源\">\r\n            <el-radio-group v-model=\"sizeForm.resource\" size=\"medium\">\r\n              <el-radio border label=\"线上品牌商赞助\"></el-radio>\r\n              <el-radio border label=\"线下场地免费\"></el-radio>\r\n            </el-radio-group>\r\n          </el-form-item>\r\n          <el-form-item size=\"large\">\r\n            <el-button type=\"primary\" @click=\"onSubmit\">立即创建</el-button>\r\n            <el-button>取消</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n\r\n\r\n      </el-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// 局部引入\r\nimport EleUploadImage from 'vue-ele-upload-image'\r\nimport axios from 'axios'\r\nexport default {\r\n  name: 'BlockStorage',\r\n  components: {\r\n    EleUploadImage\r\n  },\r\n  data() {\r\n    return {\r\n      fileList01: [{ name: 'food.jpeg', url: 'https://fuss10.elemecdn.com/3/63/4e7f3a15429bfda99bce42a18cdd1jpeg.jpeg?imageMogr2/thumbnail/360x360/format/webp/quality/100' },\r\n      { name: 'food2.jpeg', url: 'https://fuss10.elemecdn.com/3/63/4e7f3a15429bfda99bce42a18cdd1jpeg.jpeg?imageMogr2/thumbnail/360x360/format/webp/quality/100' }],\r\n      fileList02: [{ name: 'food.jpeg', url: 'https://fuss10.elemecdn.com/3/63/4e7f3a15429bfda99bce42a18cdd1jpeg.jpeg?imageMogr2/thumbnail/360x360/format/webp/quality/100' }],\r\n      headers: {\r\n        Authorization: localStorage.getItem(\"token\")\r\n      },\r\n      form: {\r\n        name: '',\r\n        region: '',\r\n        date1: '',\r\n        date2: '',\r\n        delivery: false,\r\n        type: [],\r\n        resource: '',\r\n        desc: '',\r\n        image: '',\r\n      }\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.visualizationOfBlock {\r\n  flex: 6;\r\n  border-radius: 15px;\r\n  width: 985px;\r\n  margin-left: 9%;\r\n}\r\n\r\n#el-form {\r\n  margin: 5%;\r\n}\r\n\r\n#el-divider {\r\n  height: 20px;\r\n  margin-top: 10%;\r\n  margin-bottom: 10%;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}